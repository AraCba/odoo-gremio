                        <field name="caracter_aporte" domain="[('activo', '=', 'True')]"/>
                        <field name="dependencia_aporte" domain="[('activo', '=', 'True')]"/>
                        <field name="subdependencia_aporte" domain="[('activo', '=', 'True')]"/> -->
                    </group>
                </sheet>
            </form>
        </field>
    </record>

