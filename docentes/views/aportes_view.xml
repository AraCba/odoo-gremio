    <record model="ir.ui.view" id="aportes_tree_view">
        <field name="name">Aportes</field>
        <field name="model">docentes.aportes</field>
        <field name="arch" type="xml">
            <tree string="Aportes">
                <field name="docente"/>
                <field name="legajo"/>
                <field name="fecha"/>
                <field name="codigo"/>
                <field name="aporte"/>
            </tree>
        </field>
    </record>

    <record model="ir.ui.view" id="aportes_form_view">
        <field name="name">Aportes</field>
        <field name="model">docentes.aportes</field>
        <field name="arch" type="xml">
            <form string="Aporte">
                <sheet>
                    <group>
                        <field name="docente_bis"/>
                        <!--field name="nombre"/>
                        <field name="legajo"/-->
                        <field name="fecha"/>
                        <field name="codigo"/>
                        <field name="aporte"/>
                        <!-- <field name="codigo_aporte" domain="[('activo', '=', 'True')]"/>
